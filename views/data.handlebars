<!-- original data -->

<div class='container'> 
    <div class='jumbotron'>
        <div class='col-lg-12 text-center'>
            <h1>Data</h1>
        </div>
    </div>

<hr />

    <div class='row'>
        <div class='col-lg-4 card bg-success'>
            <ul>
                <div class='card-header'>
                    {{#each JournalEntries}}
                </div>
                <div class='card-body'>
                        <li>
                            <p><span>ID:</span> {{this.dataValues.id}}</p>
                            <p><span>Date:</span> {{this.dataValues.date}}</p>
                            <p><span>Title:</span> {{this.dataValues.title}}</p>
                        </li>
                        {{#each this.ActivityEntries}}
                            <li>
                                <p><span>Name:</span> {{ActivityEntry}}</p>
                                <p><span>Total Time:</span> {{this.totalTime}}</p>
                                <p><span>Activity Notes:</span> {{this.entryActivityText}}</p>
                            </li>
                        {{/each}}
                    {{/each}}
                </div>
            </ul>
        </div>

        <div class='col-lg-4 card bg-success'>
            <ul>
                <div class='card-header'>
                    {{#each ActivityEntries}}
                </div>
                <div class='card-body'>
                        <li>
                            <p><span>Name:</span> {{this.name}}</p>
                            <p><span>Total Time:</span> {{this.totalTime}}</p>
                            <p><span>Activity Notes:</span> {{this.entryActivityText}}</p>
                        </li>
                    {{/each}}
                </div>
            </ul>
        </div>

        <div class='col-lg-4 card bg-success'>
            <ul>
                <div class='card-header'>
                    {{#each NutritionEntries}}
                </div>
                <div class='card-body'>
                        <li>
                        <p><span>Time of Meal:</span> {{this.typeOfMeal}}</p>
                        <p><span>Ate:</span> {{this.entryNutritionText}}</p>
                        </li>
                    {{/each}}
                </div>
            </ul>
        </div>

    </div>
</div>
<!-- end of data -->